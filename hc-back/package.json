    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.4",
    "ping": "^0.2.2",
    "prompts": "^1.2.0",
    "request": "^2.88.0",
    "sqlite3": "^4.0.4",
    "wake_on_lan": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
