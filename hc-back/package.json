{
  "name": "hc-back",
  "version": "1.0.0",
  "description": "Turn your servers on/off from the web.",
  "main": "index.js",
  "scripts": {
    "create-db": "./bin/create-db.js",
    "create-user": "./bin/create-user.js",
    "create-config": "./bin/create-config.js",
    "initialize": "./bin/welcome.js && npm run create-db && npm run create-user && npm run create-config",
    "bundle": "node_modules/.bin/webpack",
    "build-prod": "npm run bundle && cp -R ./bin ../dist && ./clean-package-json.js",
